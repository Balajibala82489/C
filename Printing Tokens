https://www.hackerrank.com/challenges/printing-tokens-/problem?isFullScreen=true

#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    char str[1000];
    gets(str);
    char *token = strtok(str, " ");
   
    
    while (token != NULL)
    {
        printf("%s\n", token);
        token = strtok(NULL, " ");
    }
    
    
    
    return 0;
}
